{
  "name": "@spectra/types",
  "version": "3.0.0",
  "description": "Shared TypeScript types for SpectraAI",
  "main": "index.ts",
  "types": "index.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.6.0"
  },
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    },
    "./ai": {
      "types": "./ai.ts",
      "default": "./ai.ts"
    },
    "./auth": {
      "types": "./auth.ts",
      "default": "./auth.ts"
    },
    "./memory": {
      "types": "./memory.ts",
      "default": "./memory.ts"
    }
  }
}
